{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/checkout-block",
	"version": "0.1.0",
	"title": "Checkout block",
	"category": "contribuinte-checkout-category",
	"description": "Add fiscal section to checkout.",
	"example": {},
	"supports": {
		"html": false,
		"align": false,
		"multiple": false,
		"reusable": false
	},
	"parent": [
		"woocommerce/checkout-fields-block"
	],
	"attributes": {
		"text_box_vat_field_label": {
			"type": "string"
		},
		"text_box_vat_field_description": {
			"type": "string"
		},
		"drop_down_show_step_number": {
			"type": "boolean"
		},
		"drop_down_is_required": {
			"type": "boolean"
		},
		"drop_down_required_over_limit_price": {
			"type": "boolean"
		},
		"drop_down_validate_vat": {
			"type": "boolean"
		},
		"drop_down_on_validation_fail": {
			"enum": [
				0,
				1
			]
		}
	},
	"textdomain": "contribuinte-checkout",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
