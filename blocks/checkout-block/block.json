{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/checkout-block",
	"version": "0.1.0",
	"title": "Checkout block",
	"category": "contribuinte-checkout-category",
	"description": "Add fiscal section to checkout.",
	"example": {},
	"supports": {
		"html": false,
		"align": false,
		"multiple": false,
		"reusable": false
	},
	"parent": [
		"woocommerce/checkout-fields-block"
	],
	"attributes": {
		"lock": {
			"type": "object",
			"default": {
				"remove": false,
				"move": false
			}
		},
		"showStepNumber": {
			"type": "boolean",
			"default": true
		},
		"title": {
			"type": "string",
			"default": "Tax details"
		},
		"description": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "contribuinte-checkout",
	"editorScript": "file:./index.js"
}
